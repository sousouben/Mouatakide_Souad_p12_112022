{
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs/|public)"
    },
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "encoding": "utf8",
      "destination": "./docs/"
    }
  }